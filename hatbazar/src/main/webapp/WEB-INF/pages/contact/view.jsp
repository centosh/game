<div id="viewMessage" class="modal hide fade" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-header">
        <h3 id="myModalLabel">Message Details</h3>
    </div>
    <div class="modal-body">
        <p>
        <table class="table table-striped">
            <tr><td>Send By</td><td id="sendBy">Send BY Null</td></tr>
            <tr><td>Email</td><td id="senderEmail">Sender Email Is Null</td></tr>
            <tr><td>Email</td><td id="senderPhone">Sender Phone is Null</td></tr>
            <tr><td>Phone</td><td id="senderSubject">Sender Subject is Null</td></tr>
            <tr><td colspan="3" id="senderMessage">Sender Message is Null</td></tr>
        </table>
        </p>
    </div>
    <div class="modal-footer">
        <button type="reset" class="btn btn-inverse" data-dismiss="modal" aria-hidden="true">Close</button>
    </div>
</div>
